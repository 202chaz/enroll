<a href="#<%= cell.support_text_key %>" data-toggle="modal" data-target="#<%= cell.support_text_key %>">Not Sure?</a>

<!-- Modal -->
<div class="modal fade" id="<%= cell.support_text_key %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle"><%= cell.support_text_key.titleize if cell.support_text_key.present? %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= raw @support_texts[cell.support_text_key] %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>