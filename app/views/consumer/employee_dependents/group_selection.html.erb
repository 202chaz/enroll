<div class="container-fluid no-pd-left">
	<div class="panel panel-default">
		<div class="panel-body left">
			<div class="col-md-10 col-sm-12">
				<div class="col-md-10 col-sm-12">
                                        <p class="information row">Group Selection</p>
					<%= link_to 'Continue', select_plan_people_path(person_id: params[:person_id], organization_id: params[:organization_id]), class: 'btn btn-green pull-left' %>
					&nbsp;&nbsp;
					<%= link_to 'My Account', person_my_account_path(current_user.person), class: 'btn btn-green' %>
				</div>
				<div class="clear-fix"></div>

				<div class="col-md-2 col-sm-3 col-xs-12 key no-pd-right" id="key-section">
					<%= link_to "Save & Exit", home_consumer_profiles_path, { :class => "btn btn-save" } %>
					<input type="hidden" id="initial_progress" value="60">
					          <div style="display: none;" id="sup-progress" data-signup-progress="6"></div>
					<%= render 'shared/signup_progress' %>
					<a href="#" class="btn btn-default" style="display: none;" data-uri="<%= comparison_products_plans_path %>" id="compare-selected-plans-link">Compare Selected Plans</a>
				</div>
			</div>
		</div>
	</div>
</div>
