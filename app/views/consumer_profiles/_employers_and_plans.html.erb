	                                    <h1>$249 <small>/Month</small></h1>
	                                </div>
	                                <table class="table">
	                                    <thead>
	                                        <tr>
	                                            <th>TYPE</th>
	                                            <th>LEVEL</th>
	                                            <th>NETWORK</th>
	                                            <th>DEDUCTIBLE</th>
	                                        </tr>
	                                    </thead>
	                                    <tbody>
	                                        <tr>
	                                            <td>HMO</td>
	                                            <td><%= image_tag 'icons/gold-circle.png', class: 'hidden-sm' %> Gold</td>
	                                            <td>Nationwide</td>
	                                            <td>$500</td>
	                                        </tr>
	                                    </tbody>
	                                </table>
	                            </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="panel panel-default min-h">
                            	<div class="panel-body">
                            		<h3 style="margin-top: 0"><%= @person.full_name %></h3>
                            		<p>(DC Health link ID : <%= current_user.id %>)</p>
                            		<br>
	                                <div class="page-header">
	                                    <h4>Employer</h4>
	                                    <a href="#"><span class="glyphicon glyphicon-cog" aria-hidden="true"></span> Edit</a>
	                                </div>

	                                <h3><%= @employer_profile.dba %></h3>
	                                <ul class="list-unstyled list-comp">
	                                    <li>
	                                        HIRED: <EM><%= @employee_roles.first.new_census_employee.hired_on.strftime("%m/%d/%Y") %></EM>
	                                    </li>
	                                    <li>
	                                        ELIGIBLE FOR COVERAGE: <EM><%= @employee_roles.first.effective_on.strftime("%m/%d/%Y") %></EM>
	                                    </li>
	                                </ul>
	                            </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-default">
                    	<div class="panel-body">
                    		<div id="qle-menu">
		                        <div class="page-header">
		                            <h4>Your life events</h4>
		                        </div>
		                        <div class="row">
		                        	<% if @qualifying_life_events %>		
										<% @qualifying_life_events.each_with_index do |qle, index| %>
											<% if index%5 == 0 %>
												<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
												<ul class="list-unstyled list-inline list-qle">
											<% end %>

											<li>
												<a class="qle-menu-item" href="javascript:void(0)" data-toggle="tooltip" title="<%= qle.description %>">"<%= qle.title.humanize %>"</a>
											</li>

											<% if index%5 == 4 || index+1 == @qualifying_life_events.count %>
												</ul>
												</div>
											<% end %>
										<% end %>
									<% end %>
								</div>
							</div>

							<%= render 'qles' %>
                        </div>
                    </div>
