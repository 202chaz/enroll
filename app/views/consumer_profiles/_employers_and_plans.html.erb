                    	<div class="panel-body">
                    		<div class="row">
	                            <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
	                            	<%= image_tag 'icons/family_members.png' %>
	                            </div>
	                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12 info-wrapper">
