<div aria-labelledby="home-tab" id="home" class="tab-pane fade in active" role="tabpanel">
	<div class="container">
		<div class="row">
			<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="panel panel-default min-h">
                            	<div class="panel-body">
	                                <div class="page-header">
	                                    <h4>Your plan</h4>
	                                    <a href="#"><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span> Details</a>
	                                </div>
	                                <%= image_tag 'company-logo/CareFirst.png' %>

	                                <h3>BlueCross Blueshield Preferred 500, A Multi State Plan</h3>
	                                <div class="page-header">
	                                    <h1>$249 <small>/Month</small></h1>
	                                </div>
	                                <table class="table">
	                                    <thead>
	                                        <tr>
	                                            <th>TYPE</th>
	                                            <th>LEVEL</th>
	                                            <th>NETWORK</th>
	                                            <th>DEDUCTIBLE</th>
	                                        </tr>
	                                    </thead>
	                                    <tbody>
	                                        <tr>
	                                            <td>HMO</td>
	                                            <td><%= image_tag 'icons/gold-circle.png', class: 'hidden-sm' %> Gold</td>
	                                            <td>Nationwide</td>
	                                            <td>$500</td>
	                                        </tr>
	                                    </tbody>
	                                </table>
	                            </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="panel panel-default min-h">
                            	<div class="panel-body">
                            		<h3 style="margin-top: 0"><%= @person.full_name %></h3>
                            		<p>(DC Health link ID : <%= current_user.id %>)</p>
                            		<br>
	                                <div class="page-header">
	                                    <h4>Employer</h4>
	                                    <a href="#"><span class="glyphicon glyphicon-cog" aria-hidden="true"></span> Edit</a>
	                                </div>

	                                <h3><%= @employer_profile.dba %></h3>
	                                <ul class="list-unstyled list-comp">
	                                    <li>
	                                        HIRED: <EM><%= @employee_roles.first.new_census_employee.hired_on.strftime("%m/%d/%Y") %></EM>
	                                    </li>
	                                    <li>
	                                        ELIGIBLE FOR COVERAGE: <EM><%= @employee_roles.first.effective_on.strftime("%m/%d/%Y") %></EM>
	                                    </li>
	                                </ul>
	                            </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-default">
                    	<div class="panel-body">
                    		<div id="qle-menu">
		                        <div class="page-header">
		                            <h4>Your life events</h4>
		                        </div>
		                        <div class="row">
		                        	<% if @qualifying_life_events %>		
										<% @qualifying_life_events.each_with_index do |qle, index| %>
											<% if index%5 == 0 %>
												<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
												<ul class="list-unstyled list-inline list-qle">
											<% end %>

											<li>
												<a class="qle-menu-item" href="javascript:void(0)" data-toggle="tooltip" title="<%= qle.description %>">"<%= qle.title.humanize %>"</a>
											</li>

											<% if index%5 == 4 || index+1 == @qualifying_life_events.count %>
												</ul>
												</div>
											<% end %>
										<% end %>
									<% end %>
								</div>
							</div>
							<div id="qle-details" class="hidden">
								<a class="close-popup" href="javascript:void(0);"><span aria-hidden="true" class="glyphicon glyphicon-remove"></span></a>
								<div class="row">
									
									<%= form_tag '/consumer/employee_dependents', method: :get do %>
										<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 qle-form-wrapper">
			                                <h3 class="qle-details-title">"I got married"</h3>
			                                <h5>Enter the date of the event.</h5>
			                                <%= text_field_tag 'qle_date', '', placeholder: '00-00-0000', class: 'form-control' %>
			                                <%= link_to 'Submit', 'javascript:;', class: 'btn btn-danger', id: 'qle_submit' %>
			                                <br>
			                                <a class="cancel" href="javascript:void(0);">Cancel</a>
			                            </div>

			                            <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12 qle-form-wrapper">
			                            	<div class="success-info hidden">
												<p class="success-text"><strong>You may be eligible for a special enrollment period.</strong></p>

												<%= hidden_field_tag 'change_plan' %>
												<%= hidden_field_tag 'employee_role_id', @employee_role.id %>

												<%= submit_tag 'Continue' %>
											</div>
											<div class="error-info hidden">
												<p class="error-text">The date you submitted does not qualify for special enrollment.<br>Please double check the date or contact DCHBX support 1-800-555-1212.</p>

												<button type="submit" class="btn">Continue</button>
											</div>
											<div class="default-info">
												<center>
													<strong id="qle-input-info">Please enter a date to the left.</strong><br><br>
													<button type="submit" class="btn">Continue</button>
												</center>
											</div>
		                            	</div>
		                            <% end %>

		                        </div>
							</div>
                        </div>
                    </div>
                </div>

