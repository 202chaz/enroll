<div aria-labelledby="home-tab" id="home" class="tab-pane fade in active" role="tabpanel">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="panel panel-default min-h">
                              <div class="panel-body">
                                  <div class="page-header">
                                      <h4>Your plan</h4>
                                  </div>
                                  <% plan = @hbx_enrollments.first.try(:plan) %>
                                  <% hbx_enrollment = @hbx_enrollments.first %>
                                  <%= plan.try(:carrier_profile).try(:legal_name) %>

                                  <h3><%= plan.try(:name) %></h3>
                                  <div>
                                    Subscriber Id : <%= hbx_enrollment.try(:subscriber).try(:id).try(:to_s) %>
                                    <br/>
                                    Group Id : <%= hbx_enrollment.try(:benefit_group_id).try(:to_s) %>
                                  </div>
                                  <br/>
                                  <table class="table">
                                      <thead>
                                          <tr>
                                              <th>TYPE</th>
                                              <th>LEVEL</th>
                                              <th>START</th>
                                              <th></th>
                                          </tr>
                                      </thead>
                                      <tbody>
                                          <tr>
                                              <td><%= plan.try(:plan_type).try(:upcase) %></td>
                                              <td><%= image_tag 'icons/gold-circle-2.png', class: 'hidden-sm' %> Gold</td>
                                              <td><%= hbx_enrollment.try(:effective_on) %></td>
                                              <td></td>
                                          </tr>
                                      </tbody>
                                  </table>
                              </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="panel panel-default min-h">
                              <div class="panel-body">
                                <h3 style="margin-top: 0"><%= @person.full_name %></h3>
                                <p>(DC Health link ID : <%= current_user.id %>)</p>
                                <br>
                                  <div class="page-header">
                                      <h4>Employer</h4>
                                  </div>

                                  <h3><%= @employer_profile.dba %></h3>
                                  <ul class="list-unstyled list-comp">
                                      <li>
                                          HIRED: <EM><%= @employee_role.hired_on.strftime("%m/%d/%Y") %></EM>
                                      </li>
                                      <li>
                                          ELIGIBLE FOR COVERAGE: <EM><%= @employee_role.effective_on.strftime("%m/%d/%Y") %></EM>
                                      </li>
                                  </ul>
                              </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-default">
                      <div class="panel-body">
              <%= render 'qles' %>
                        </div>
                    </div>
