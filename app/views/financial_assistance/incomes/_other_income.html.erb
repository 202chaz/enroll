<div class="focus_effect_module income-display-container-<%= income.id %>">
  <div class="row row-form-wrapper no-buffer row-height">
    <div class="col-md-2">
      <label>AMOUNT *</label>
      </br>
      <span> <%= income.amount %></span>
    </div>
     <div class="col-md-3">
      <label>How Often *</label></br>
      <span><%= income.frequency_kind %></span>
    </div>
     <div class="col-md-3">
      <label>FROM</label></br>
      <span><%= income.start_on %></span>
    </div>
     <div class="col-md-3">
       <label>TO</label></br>
      <span><%= income.end_on %></span>
    </div>
    <div class="col-md-1">
      <span class="edit-income glyphicon" data-income-id="<%= income.id %>" data-application-id="<%= @application.id %>"" data-applicant-id="<%= @applicant.id %>" style="cursor: pointer;">&#xe065;</span>
    </div>
  </div>
</div>