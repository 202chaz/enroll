<div id="<%= dom_id other_income %>" class="other-income">
  <div class="focus_effect_module other-income-show"">
    <div class="row row-form-wrapper no-buffer row-height">
      <div class="col-md-3  no-pd form-group-lg no-pd class-fa-household">
        <label class="static_label label-floatlabel">AMOUNT *</label>
        <span class="field_value floatlabel form-control active-floatlabel" id="name"><%= other_income.amount %></span>
      </div>
      <div class="col-md-3  no-pd form-group-lg no-pd class-fa-household">
        <label class="static_label label-floatlabel">HOW OFTEN *</label>
        <span class="field_value floatlabel form-control active-floatlabel" id="name"><%= other_income.frequency_kind %></span>
      </div>
      <div class="col-md-3  no-pd form-group-lg no-pd class-fa-household">
        <label class="static_label label-floatlabel">START *</label>
        <span class="field_value floatlabel form-control active-floatlabel" id="name"><%= other_income.start_on %></span>
      </div>
      <div class="col-md-2  no-pd form-group-lg no-pd class-fa-household">
        <label class="static_label label-floatlabel">END</label>
        <span class="field_value floatlabel form-control active-floatlabel" id="name"><%= other_income.end_on %></span>
      </div>
      <div class="col-md-1 no-pd form-group-lg no-pd class-fa-household">
        <a class="other-income-edit"><i class="fa fa-pencil" aria-hidden="true"></i></a>
        <a class="other-income-delete"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
      </div>
    </div>
  </div>

  <div class="edit-other-income-form hidden">
   <%= render partial: 'financial_assistance/incomes/other_income_form', locals: { other_income: other_income, disabled: true, kind: other_income.kind } %>
 </div>
</div>
