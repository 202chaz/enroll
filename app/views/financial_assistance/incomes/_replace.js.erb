stopEditingIncome();
//need to dynamcially handle different income types
<% if @income.kind == 'wages_and_salaries' %>
  $('<%= id %>').parent().append("<%= escape_javascript render(partial: 'job_income', locals: { income: @income }) %>");
  $('<%= id %>').remove();
<% elsif @income.kind == 'self_employed_income' || @income.kind == 'net_self_employment'%>
  $('<%= id %>').parent().append("<%= escape_javascript render(partial: 'self_employed_income', locals: { income: @income }) %>");
  $('<%= id %>').remove();
<% else %>
	// handle other income here!
	stopEditingIncome();
	$("<%= id %>").html("<%==j render(partial: 'other_income', locals: { other_income: @income, kind: @income.kind}) %>");
	$("<%= id %>").closest(".other-income-kind").find('.add-more-link-<%= @income.kind %> > a').removeClass("hidden");
<% end %>