<div class="col-md-10">
  <div class="row">
    <div class="col-md-6">
      <h3 class="title-inline"><b>Add Incomes</b></h3>
    </div>
    <div class="col-md-6">
      <div> <%= link_to "Go To Household Member Page", insured_family_members_path, class: "btn btn-small btn-default back_to-household-btn" %></div>
    </div>
  </div>

  <div>
    <% if @applicant.incomes.present? %>
      <h4>Edit Existing Income(s)</h4>
      <table class="table">
        <th>Income Type</th>
        <th></th>
        <th></th>
        <% @applicant.incomes.each do |income| %>
        <tr>
          <td><%=income.kind%></td>
          <td><%= link_to "Edit", go_to_step_financial_assistance_application_applicant_income_path(@application, @applicant, income, 1), class: "btn btn-small btn-success" %></td>
          <td><%= link_to "Delete", financial_assistance_application_applicant_income_path(@application, @applicant, income), method: :delete, class: "btn btn-small btn-warning", data: { confirm: "Are you sure?" }%></td>
        </tr>
        <% end %>
      </table>
    <%end%>
  </ div>
  <%= link_to "Add Income", new_financial_assistance_application_applicant_income_path(@application, @applicant) %>
</div>
