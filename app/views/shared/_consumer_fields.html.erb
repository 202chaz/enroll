<div id="consumer_fields" class="focus_effect">

  <div class="row row-form-wrapper no-buffer">

    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-group form-group-lg no-pd">
      Are you a US Citizen or US National?
    </div>

    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <div class="radio skinned-form-controls skinned-form-controls-mac">
        <%= radio_button_tag :citizen_status, "yes", class: "required floatlabel" %>
        <label for="citizen_status_yes"><span>Yes</span></label>
      </div>
    </div>
    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <div class="radio skinned-form-controls skinned-form-controls-mac">
        <%= radio_button_tag :citizen_status, "no", class: "required floatlabel" %>
        <label for="citizen_status_no"><span>No</span></label>
      </div>
    </div>

  </div>

  <div class="row row-form-wrapper no-buffer" id="naturalized_citizen_container" style="display: none">

    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-group form-group-lg no-pd">
      Are you a naturalized citizen?
    </div>

    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <div class="radio skinned-form-controls skinned-form-controls-mac">
        <%= radio_button_tag :naturalized_citizen, "yes", class: "required floatlabel" %>
        <label for="naturalized_citizen_yes"><span>Yes</span></label>
      </div>
    </div>
    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <div class="radio skinned-form-controls skinned-form-controls-mac">
        <%= radio_button_tag :naturalized_citizen, "no", checked: "no"  , class: "required floatlabel" %>
        <label for="naturalized_citizen_no"><span>No</span></label>
      </div>
    </div>
  </div>

  <div class="row row-form-wrapper no-buffer" id="immigration_status_container" style="display: none">
    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      Do you have eligible immigration status?
    </div>
    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <div class="radio skinned-form-controls skinned-form-controls-mac">
        <%= radio_button_tag :immigration_status, "yes", checked: true, class: "required floatlabel" %>
        <label for="immigration_status_yes"><span>Yes</span></label>
      </div>
    </div>
    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <div class="radio skinned-form-controls skinned-form-controls-mac">
        <%= radio_button_tag :immigration_status, "no", checked: false, class: "required floatlabel" %>
        <label for="immigration_status_no"><span>No</span></label>
      </div>
    </div>
  </div>

  <div class="row row-form-wrapper no-buffer" id="vlp_document_id_container" style="display: none">

    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      Select your document type
    </div>

    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <%= select_tag :naturalization_doc_type, options_for_select(ConsumerRole.naturalization_document_types) %>
    </div>

    <div class="col-md-4 col-sm-4 col-xs-6 form-group form-group-lg no-pd">
      <%= f.fields_for :consumer_role do |cr| %>
          <%= cr.text_field :vlp_document_id, :placeholder => "Document Id" %>
      <% end %>
    </div>
  </div>


  <div class="row row-form-wrapper no-buffer" id="vlp_document_id_container">

    <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12 form-group form-group-lg no-pd">
      Are you a member of an American Indian tribe or Alaskan Native?
    </div>
    <div style="text-align: center" class="col-lg-1 col-md-1 col-sm-1 col-xs-12 form-group form-group-lg no-pd">

      <%= check_box_tag :american_indian_tribe %>
    </div>

    <div id="tribal_container" style="display: none">
      <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
        <%= text_field_tag :tribal_id, nil, class: "required floatlabel form-control", placeholder: 'TRIBAL ID' %>
      </div>

      <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
        <%= file_field_tag :tribal_document, class: "required floatlabel form-control" %>
      </div>
    </div>
  </div>
</div>

<div class="row row-form-wrapper no-buffer" id="vlp_document_id_container">
  <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12 form-group form-group-lg no-pd">
