<div class="row row-form-wrapper no-buffer">
<fieldset style="<%= 'display: none;' if f.object.relationship == 'child_26_and_over' %>">
  <div class="col-xs-6 form-group form-group-lg">
    <div class="checkbox">
      <label>
        <%= f.check_box :offered, {class: "contribution_handler", disabled: (f.object.relationship == 'employee' ? true:false)}, "true", "false" %>
        <%= f.label :offered do %>

        <i class="fa fa-check-square-o fa-2x"></i>
        <i class="fa fa-square-o fa-2x"></i>

        <% end %>

        <span>
          <%= f.hidden_field :relationship, value: f.object.relationship %>
          <p><%= f.object.relationship.try(:humanize) %></p>
        </span>
      </label>
    </div>
  </div>

      <div class="col-xs-6 form-group form-group-lg">
        <%= f.number_field :premium_pct, class: "contribution_handler slider", value: f.object.premium_pct.try(:to_i), :data => { :slider_min => '0', :slider_max => '100', :slider_value => '0' } %>
&nbsp;&nbsp;&nbsp;<span class="slide-label">0</span>%</span><!--<i class="fa fa-question-circle fa-lg"></i>-->
</div>
</fieldset>
</div>
