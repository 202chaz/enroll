<div id="email_info" class="focus_effect row personaol-info-row" style="opacity: 0.5;">
	<div class="email">
		<div class="col-md-10 col-sm-9 no-pd first">
		  <div class="form-group form-group-lg">
        <%= f.hidden_field :kind %>
        <% kind = f.object.kind.upcase %>
		  	<%= f.text_field :address, placeholder: "NAME@#{kind}.COM", class: "floatlabel form-control" %>
		  </div>
		  <div class="error">
		    <div class="warning"><i class="fa fa-exclamation-triangle"></i>
		      All fields Required</div>
		  </div>
		</div>
	</div>
</div>