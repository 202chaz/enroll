<div class="row">
<% if current_user.blank? || current_user.person_id.blank? %>
	<div class="col-md-9 col-sm-9 col-xs-12">
		<h4>Personal Information</h4>
		<div class="row no-buffer row-form-wrapper">
			<div class="col-md-4 col-sm-4 col-xs-12 form-group-lg no-pd">
				<%= f.text_field :first_name, class: "floatlabel form-control", required: true, placeholder: 'FIRST NAME' %>
			</div>
			<div class="col-md-4 col-sm-4 col-xs-12 form-group-lg no-pd">
				<%= f.text_field :last_name, class: "floatlabel form-control", required: true, placeholder: 'LAST NAME' %>
			</div>
			<div class="col-md-4 col-sm-4 col-xs-12 form-group-lg no-pd">
              <%= datepicker_control f, :dob, {:class => "floatlabel form-control", :placeholder => "DATE OF BIRTH controll", :'data-date-min' => "-110y", :'data-date-max' => "+0", :'data-date-format' => 'yy-mm-dd'} %>
			</div>
		</div>
	</div>
  <script type="text/javascript">
  $(function() {
    applyJQDatePickers();
  });
  </script>
<% else %>
<%= f.hidden_field :person_id, value: current_user.person_id %>
<% end %>
</div>
