<div class="row top-row">
  <div class="col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :first_name, class: "required floatlabel form-control", required: true, placeholder: 'FIRST NAME' %>
  </div>
  <div class="col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :middle_name, class: "floatlabel form-control", placeholder: 'MIDDLE NAME' %>
  </div>
  <div class="col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :last_name, class: "required floatlabel form-control", required: true, placeholder: 'LAST NAME' %>
  </div>
  <div class="col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :name_sfx, class: "floatlabel form-control", placeholder: 'SUFFIX' %>
  </div>
</div>

<div class="row bottom-row form-group form-group-lg no-pd">
  <div class="col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :dob, start_year: Date.today.year - 110, end_year: Date.today.year, placeholder: "BIRTHDATE",
        class: "required date_picker floatlabel form-control", required: true, value: f.object.dob.try(:strftime, "%m/%d/%Y") %>
  </div>
  <div class="col-md-3 col-sm-3 col-xs-12 form-group form-group-lg no-pd">
    <%= f.text_field :ssn, placeholder: "SOCIAL SECURITY", class: "required floatlabel form-control", value: number_to_ssn(f.object.ssn), required: true, data: {:mask => "99999999"}   %>
  </div>
  <div class="col-md-3 col-sm-3 col-xs-6 form-group form-group-lg no-pd">
    <div class="radio skinned-form-controls skinned-form-controls-mac">
      <label>
        <%= f.radio_button :gender, "male", required: true, class: "required floatlabel" %>
        <span>Male</span>
      </label>
    </div>
  </div>
  <div class="col-md-3 col-sm-3 col-xs-6 form-group form-group-lg no-pd">
    <div class="radio skinned-form-controls skinned-form-controls-mac">
      <label>
        <%= f.radio_button :gender, "female",class: "required floatlabel"  %>
        <span>Female</span>
      </label>
    </div>
  </div>
</div>
