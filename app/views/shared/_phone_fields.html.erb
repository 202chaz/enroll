<div id="phone_info" class="focus_effect row personaol-info-row" style="opacity: 0.5;">
  <div class="phone col-md-10 col-sm-9 col-xs-12 no-pd first">
    
    <%= f.fields_for :phones, errors: {}, fieldset: true do |phone|  %>
    <div class="row top-row">
      <div class="col-md-2 col-sm-2 no-pd">
        <div class="form-group form-group-lg">
          <% kind = phone.object.kind.upcase %>
          <%= phone.hidden_field :kind %>
          <%= phone.text_field :area_code, placeholder: "AREACODE", class: "small-textfield floatlabel form-control border-bottom-none border-right-none" %>
        </div>
      </div>
      <div class="col-md-10 col-sm-10 no-pd">
        <div class="form-group form-group-lg">
          <% kind = phone.object.kind.upcase %>
          <%= phone.text_field :number, placeholder: kind, class: "floatlabel form-control border-bottom-none border-right-none" %>
        </div>
      </div>
    </div>
    <% end%>
    
    <div class="error">
      <div class="warning"><i class="fa fa-exclamation-triangle"></i>
        All fields Required</div>
    </div>
  </div>
</div>