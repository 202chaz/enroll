<div id="phone_info" class="focus_effect row personaol-info-row" style="opacity: 0.5;">
  <div class="phone col-md-10 col-sm-9 col-xs-12 no-pd first">
    <div class="row top-row">
      <div class="col-md-6 col-sm-6 no-pd">
        <div class="form-group form-group-lg">
          <%= f.text_field :number, placeholder: 'HOME', class: "floatlabel form-control border-bottom-none border-right-none" %>
        </div>  
      </div>
      <div class="col-md-6 col-sm-6 no-pd">
        <div class="form-group form-group-lg">
          <%= f.text_field :number, placeholder: 'BUSINESS', class: "floatlabel form-control border-bottom-none" %>
        </div>  
      </div>
    </div>
    <div class="row bottom-row">
      <div class="col-md-6 col-sm-6 no-pd">
        <div class="form-group form-group-lg">
          <%= f.text_field :number, placeholder: 'CELL', class: "floatlabel form-control border-right-none" %>
        </div>  
      </div>
      <div class="col-md-6 col-sm-6 no-pd">
        <div class="form-group form-group-lg">
          <%= f.text_field :number, placeholder: 'FAX', class: "floatlabel form-control" %>
        </div>  
      </div>
    </div>
    <div class="error">
      <div class="warning"><i class="fa fa-exclamation-triangle"></i>
        All fields Required</div>
    </div>
  </div>
</div>