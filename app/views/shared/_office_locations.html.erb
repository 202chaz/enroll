<%= f.fields_for :office_locations, errors: {}, fieldset: false do |office_location| %>
<h4> Address </h4>
<%= render partial: 'shared/office_location_fields', locals: {f: office_location} %>
<% end %>

<div class="form-inputs" id="office_locations_buttons">
	<%= link_to('<i class="fa fa-user-plus"></i> Add Office Location'.html_safe, new_office_location_path(:child_index => f.object.office_locations.length, :parent_object => f.object_name), remote: true, class: 'fui-plus-circle') %>
  <br/>
</div>
