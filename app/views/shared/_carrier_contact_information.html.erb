<div class="modal fade carrier-contact" id="<%= plan.coverage_kind %>-<%= plan.id %>" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button class="close" data-dismiss="modal" type="button">&times;</button>
        <h4 class="modal-title"><%= plan.carrier_profile.legal_name %>
          Customer Service Line</h4>
      </div>
      <div class="modal-body">
        <i class="fa fa-phone fa-3x"></i>
        <% if plan.coverage_kind == 'health' %>
        <div class="carrier-numbers">
          <% if plan.carrier_profile.legal_name == "CareFirst" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong>
          </div>
          <% elsif plan.carrier_profile.legal_name == "Aetna" %>
          <div>
            <strong>For Customers Enrolled in Individual and Family Plans</strong><br/>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.where(kind:"individual").first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.where(kind:"individual").first.us_formatted_number %></a>
            </strong><br/>
            from 8am-6pm EST, Monday - Friday
            <br/>
            <br/>
            <strong>For Brokers and Employers</strong><br/>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.where(kind:"brokers").first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.where(kind:"brokers").first.us_formatted_number %></a>
            </strong><br/>
            from 8am-5pm EST, Monday - Friday
            <br/>
            <br/>

            <strong>For Small Business Employees</strong><br/>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.where(kind:"employers").first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.where(kind:"employers").first.us_formatted_number %></a>
            </strong><br/>
            from 8am-6pm EST, Monday - Friday
            <br/>
          </div>
          <% elsif plan.carrier_profile.legal_name == "Kaiser" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong>
          </div>
          <% elsif plan.carrier_profile.legal_name == "UnitedHealthcare" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong><br/>
          </div>
          <% end %>
        </div>
        <% else %>
        <div class="carrier-numbers">
          <% if plan.carrier_profile.legal_name == "CareFirst" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong><br/>
          </div>
          <% elsif plan.carrier_profile.legal_name == "Delta Dental" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.where(kind:"main").first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.where(kind:"main").first.us_formatted_number %></a>
            </strong>
            or
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.where(kind:"main2").first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.where(kind:"main2").first.us_formatted_number %></a>
            </strong>
            <br/>
          </div>
          <% elsif plan.carrier_profile.legal_name == "MetLife" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong><br/>
          </div>
          <% elsif plan.carrier_profile.legal_name == "Dentegra" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong><br/>
          </div>
          <% elsif plan.carrier_profile.legal_name == "Dominion" %>
          <div>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong><br/>
          </div>
          <% elsif plan.carrier_profile.legal_name == "BestLife" %>
          <div><br/>
            Call:
            <strong>
              <a href="tel:<%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %>"><%=plan.carrier_profile.carrier_contacts.first.us_formatted_number %></a>
            </strong><br/>
          </div>
          <% end %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
