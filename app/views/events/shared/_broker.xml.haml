- if hbx_enrollment.broker_agency_account.writing_agent
  %broker
    %id
      %id=hbx_enrollment.broker_agency_account.writing_agent.npn
    %name= hbx_enrollment.broker_agency_account.writing_agent.person.full_name
    - if !hbx_enrollment.broker_agency_account.broker_agency_profile.dba.blank?
      %dba= hbx_enrollment.broker_agency_account.broker_agency_profile.dba
    %display_name= hbx_enrollment.broker_agency_account.broker_agency_profile.legal_name
    - if hbx_enrollment.is_shop? && hbx_enrollment.employer_profile.general_agency_profile.present?
      %tpa
        %tpa_name= hbx_enrollment.employer_profile.general_agency_profile.legal_name
        %tpa_fein= hbx_enrollment.employer_profile.general_agency_profile.fein
    %is_active= hbx_enrollment.broker_agency_account.broker_agency_profile.is_active?
