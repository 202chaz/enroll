<%= render "shared/organization_staff_search", f: f %>  

<div class="row">
  <div class="col-md-9 col-sm-9 col-xs-12">
    <div class="row no-buffer row-form-wrapper">
      <div class="col-md-6 col-sm-4 col-xs-12 form-group-lg no-pd">
        <%= f.text_field :email, class: "floatlabel form-control", placeholder: 'EMAIL' %>
      </div>
      <% if @filter == 'broker_role' %>
        <div class="col-md-6 col-sm-4 col-xs-12 form-group-lg no-pd">
          <%= f.text_field :npn, class: "floatlabel form-control", placeholder: 'NPN' %>
        </div>
      <% end %>
    </div>
  </div>
</div>