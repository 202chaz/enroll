{
  "draw" : <%= @draw %>,
  "recordsFiltered": <%= @total_records %>,
  "recordsTotal": <%= @total_records %>,
  "data" : [
  <% record_count = @broker_agency_profiles.count %>
  <% @broker_agency_profiles.each_with_index do |ba, idx| %>
     {
       "legal_name": <%= raw link_to(ba.legal_name, broker_agencies_profile_path(ba)).to_json %>,
       "dba" : <%= raw ba.dba.to_json %>,
       "fein" : <%= raw ba.fein.to_json %>,
       "entity_kind" : <%= raw ba.entity_kind.titleize.to_json %>,
       "market_kind" : <%= raw ba.market_kind.titleize.to_json %>,
       "secure_message" : <%= raw link_to('Send Secure Message'.html_safe,
                              new_broker_agencies_inbox_path(id: ba.id, profile_id: @profile._id), class: "btn btn-default" + pundit_class(HbxProfile, :modify_admin_tabs?), remote: true).to_json %>
     }<%= (idx == (record_count - 1)) ? "" : "," %>
  <% end %>
  ]
}
