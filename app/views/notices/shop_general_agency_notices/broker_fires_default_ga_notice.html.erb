<%= render partial: 'notices/shared/cover_page', :locals => {notice: notice} %>
<%= render partial: 'notices/shared/logo_and_address_shop' %>
<%= render partial: "notices/shared/shop_address", :locals => {notice: notice} %>

<table class="total_table" border="0">
  <tbody>
    <tr>
      <td style="padding-top: 10px"><strong>Email notification sent to:</strong> <%= notice.email %></h3></td>
    </tr>
  </tbody>
</table>

<%= render partial: "notices/shared/date", :locals => {notice: notice} %>

<table class="total_table" border="0">
  <tbody>
    <tr>
      <td style="padding-top: 10px"><strong><%= notice.general_agency.broker_fullname.upcase %> HAS REMOVED YOU AS THEIR DEFAULT GENERAL AGENCY</strong></td>
    </tr>
  </tbody>
</table>

<table class="total_table" border="0">
  <tbody>
    <tr>
      <td class="notice-font" style="padding-top: 10px">Dear <strong><%= notice.general_agency_name %>,</strong></td>
    </tr>
  </tbody>
</table>

<%= render partial: 'notices/shared/paragraph', locals: {
 content: "This notice is to inform you that <strong>#{notice.general_agency_name}</strong> has been removed as the default general agency for <strong>#{notice.general_agency.broker_fullname}</strong> as of <strong>#{TimeKeeper.date_of_record}.</strong> You will no longer be authorized to access <strong>#{notice.general_agency.broker_fullname}</strong>â€™s #{Settings.site.short_name} Account or take action on behalf of any clients of <strong>#{notice.general_agency.broker_fullname}</strong>. If you were assigned as the General Agency for any clients of <strong>#{notice.general_agency.broker_fullname}</strong>, you will receive a separate notification specific to those groups."} %>
<br>

<%= render partial: 'notices/shared/paragraph_with_headers', :locals => {
    contents: ["Please contact #{Settings.site.short_name} with any questions:"],
    heading: 'For Questions or Assistance:', sub_heading: ''} %>

<%= render partial: 'notices/shared/unordered_list', :locals => { listings:
  ["By calling #{notice.hbe.phone}",
    "TTY: #{Settings.contact_center.tty}",
    "Online at <a href=#{Settings.site.home_url}>#{Settings.site.website_name}</a>"]} %>
