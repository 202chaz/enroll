<div class='content'>
   <div class='container top-pd'>
       <h4> <%= @title%> </h4>
       <% if current_user.has_csr_role? && !@cac%>
         <%= render 'consumer_application_links' %>
       <% end %>
       <br/>
       <div class='col-md-12 col-lg-12'>
         <p> After working on an enrollment return to this page via the person icon in the header </p>
         <br/>
       </div>
        <br/>
        <div>
            <div id='inbox'>
            <div class="row col-md-10">
               <%=render 'message_list', provider: current_user.person %>
            </div>
           </div>
        </div>
    </div>
</div>
