<div class="right-side">
  <div class="col-md-10 col-xs-12 col-sm-10 no-pd">
    <ul class="list-group">
      <% @employee.employee_family.census_dependents.each do |dependent| %>
      <li class="list-group-item">
        <div class="row">
          <div class="col-md-10 col-sm-8 col-xs-8">
            <h4> <%= dependent.first_name %> <%= dependent.last_name %> </h4>
          </div>
          <div class="col-md-2 col-sm-4 col-xs-4 text-right">
            <a href="#"><%= image_tag 'edit.png' %></a>
            <a href="#"><%= image_tag 'member-close.png' %></a>
          </div>
        </div>
      </li>
      <% end %>
    </ul>
  </div>
  <div class="col-md-2 col-xs-12 col-sm-2">
    <button class="btn">New Member</button>
  </div>
</div>