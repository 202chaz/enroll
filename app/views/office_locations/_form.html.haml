= fields_for(@parent_object, @slug_owner, :fieldset => false) do |f|
  = f.fields_for("office_locations", @office_location, child_index: @child_index) do |builder|
    %h4 Address
    = render("shared/office_location_fields", f: builder)

.form-inputs#office_locations_buttons
  = link_to('<i class="fa fa-user-plus"></i> Add Office Location'.html_safe, new_office_location_path(:child_index => @child_index.to_i + 1, :parent_object => @parent_object), remote: true, class: 'fui-plus-circle')
