<br>
<div class="container">
  <div class="row no-buffer">
    <h3 class="title-inline">Premium Billing Report <%= @current_plan_year.start_on %> - <%= @current_plan_year.start_on.end_of_month %></h3>
    <a href="#" class="btn btn-danger return_to_home pull-right bottom-mgn-xs"><i class="fa fa-times"></i> Cancel</a>
  </div>

  <div class="row no-buffer table-responsive">
    <table class="table table-bg-white table-employee-2">
      <thead>
        <tr>
          <th>Employee</th>
          <th>SSN</th>
          <th>DOB</th>
          <th>Hired</th>
          <th>Benefit Group</th>
          <th>Type</th>
          <th>Name</th>
          <th>Issuer</th>
          <th style="text-align: center;">Covered Ct</th>
          <th style="text-align: center;">Employer Contribution</th>
          <th style="text-align: center;">Employee Premium</th>
          <th style="text-align: center;">Total Premium</th>
        </tr>
      </thead>
      <tbody>
        <% @hbx_enrollments.each do |hbx_enrollment| %>
          <%= render "employee_line_item", census_employee: hbx_enrollment.subscriber.person.employee_roles.first.census_employee, enrollment: hbx_enrollment %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>