<div class="container">
	<div class="row">
		<div class="col-md-9 col-sm-9 col-xs-12">
			<h4>Personal Information</h4>
			<div class="row no-buffer row-form-wrapper">
				<div class="col-md-4 col-sm-4 col-xs-12 form-group-lg no-pd">
					<%= f.text_field :first_name, class: "floatlabel form-control", placeholder: 'FIRST NAME' %>
				</div>
				<div class="col-md-4 col-sm-4 col-xs-12 form-group-lg no-pd border_bottom_zero">
					<%= f.text_field :last_name, class: "floatlabel form-control", placeholder: 'LAST NAME' %>
				</div>
				<div class="col-md-4 col-sm-4 col-xs-12 form-group-lg no-pd">
					<%= datepicker_control f, :dob, {:class => "floatlabel form-control", :placeholder => "DATE OF BIRTH"} %>
				</div>
			</div>
		</div>
	</div><br>
		
	<div class="row">
		<div class="col-md-9 col-sm-9 col-xs-12">
			<h4>Employer Information</h4>
			<div class="row no-buffer row-form-wrapper">
				<%= render "shared/organizations/entity_name_and_type_fields", f: f %>
			</div><br>

			<%= render "shared/organizations/office_location_fields", f: f %>

			<div class="row no-buffer">
				<button type="submit" class="btn btn-lg btn-tranparent">Create Employer</button>
			</div>
		</div>
	</div><br>
</div>
