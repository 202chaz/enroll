<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <p>If you don't see your info below, click here for help.</p>
  </div>
</div>
<% Array(@person).each do |person| %>
  <%= form_for(person, :url => person_create_employers_employer_profiles_path, :as => :person, :method => :post, :remote => true) do |f| %>
    <div class="row">
      <div class="col-md-6 col-sm-6 col-xs-12">
        <dl class="dl-horizontal">
          <dt>First Name</dt>
          <dd>
            <%= person.first_name %>
          </dd>
          <dt>Last Name</dt>
          <dd>
            <%= person.last_name %>
          </dd>
          <dt>SSN</dt>
          <dd>
            <%= person.ssn %>
          </dd>
          <dt>Dob</dt>
          <dd>
            <%= person.dob %>
          </dd>
        </dl>
      </div>
      <div class="col-md-2 col-sm-2 col-xs-12">
        &nbsp;
      </div>
      <div class="col-md-4 col-sm-4 col-xs-12 no-pd text-right">
        <%= f.submit "This is my info", :class => "btn btn-primary" %>
        <%= f.hidden_field :first_name %>
        <%= f.hidden_field :last_name %>
        <%= f.hidden_field :ssn %>
        <%= f.hidden_field :dob %>
      </div>
    </div>
  <% end %>
<% end %>
