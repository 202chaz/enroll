<div class="container">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <h3>Broker Agencies</h3>

    <div class="row no-buffer">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 search-wp">
        <%= render 'shared/search', url: employers_employer_profiles_path, q: @q, placeholder: 'Name' %>
      </div>
<!--       <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 no-pd">
        <%= link_to '<i class="fa fa-user-plus"></i> New Employer Profile'.html_safe, new_employers_employer_profile_path, class: "btn btn-primary pull-right new_emp_button" %>
      </div> -->
    </div>

    <div class="table-responsive">
      <table class="table table-wrapper" style="opacity: 1;">
        <thead>
          <tr>
            <th>Agency Name</th>
            <th>Broker Name</th>
            <th>NPN</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @broker_agency_profiles.each do |agency_profile| %>
            <tr>
              <td><%= agency_profile.legal_name %></td>
              <td><%= agency_profile.primary_broker_role.person.full_name %></td>
              <td><%=  %></td>
<!--               <td><%= agency_profile.primary_broker_role.person.phones.first.to_s %></td>
              <td><%= agency_profile.primary_broker_role.person.emails.first.address %></td> -->
              <td>     
                <%= link_to 'Show'.html_safe, (broker_agencies_profile_path(agency_profile)), class: "btn btn-success", remote: true %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <%= render 'shared/alph_paginate', url: employers_employer_profile_broker_agency_index_path, alphs: @page_alphabets, remote: true %>
    </div>
  </div>
</div>