$('#<%= @location_id %> <%= @coverage_type %> .offered-plan').html("<%= escape_javascript(render(partial: "employers/plan_years/plan_selection/offered_plan")).html_safe %>")
$('#<%= @location_id %> <%= @coverage_type %> .currently-offered').html('You currently offer <%= @plan.name %>. To change your reference plan, begin by selecting "Change Plan?" to the right and then select your plan options.');

$('#<%= @location_id %> <%= @coverage_type %> .currently-offering').html('You currently offer <%= @plan.name %>. You can reselect your reference plan below, or click "Cancel" to keep your existing plan.');

$("#<%= @location_id %> <%= @coverage_type %> .offered-plan h2.employer-monthly-cost").html("<%= number_to_currency(@employer_contribution_amount.to_f) %>");
$("#<%= @location_id %> <%= @coverage_type %> .offered-plan .employee-monthly-cost-min").html("<%= number_to_currency(@min_employee_cost.to_f) %>");
$("#<%= @location_id %> <%= @coverage_type %> .offered-plan .employee-monthly-cost-max").html("<%= number_to_currency(@max_employee_cost.to_f) %>");

