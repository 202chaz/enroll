<div>
  <h3>Employee Information</h3>
</div>
<div>
  <%= employee.input :first_name, placeholder: "First", label: "First Name"  %> <br/>
  <%= employee.input :middle_name, placeholder: "Mid", label: "Middle Name"  %> <br/>
  <%= employee.input :last_name, placeholder: "Last", label: "Last Name"  %> <br/>
  <%= employee.input :name_sfx, placeholder: "Sfx", label: "Suffix" %> <br/>
  <%= employee.input :gender, label: "Gender", collection: EmployerCensus::Member::GENDER_KINDS, include_blank: "Select" %> <br/>
  <%= employee.input :ssn, as: :string, title: "SSN", placeholder: "SSN", input_html: { value: "", maxlength: 11 }, label: "Social Security" %> <br/>
  <%= employee.input :dob, as: :string,
    start_year: Date.today.year - 110,
    end_year: Date.today.year,
    placeholder: "mm/dd/yyyy",
    class: "date_picker",
    value: "" %>
  <%= employee.input :date_of_hire, as: :string, placeholder: "mm/dd/yyyy", class: "date_picker", value: "" %>
  <br/>
</div>