<fieldset>
  <div class="row">
    <p class="text-right col-md-6 col-md-push-6">
      <%= link_to '<span class="fui-cross-circle"> Delete Dependent</span>'.html_safe, '#', :class => "remove_fields text-danger" %>
    </p>
  </div>

  <%= f.text_field :first_name, placeholder: "First" %> <br/>
  <%= f.text_field :last_name, placeholder: "Last" %> <br/>
  <%= f.text_field :name_sfx, placeholder: "Sfx" %> <br/>
  <%= f.text_field :dob, as: :string,
    start_year: Date.today.year - 110,
    end_year: Date.today.year,
    placeholder: "mm/dd/yyyy",
    class: "date_picker",
    value: "" %><br/>
  <%= f.select :gender, EmployerCensus::Member::GENDER_KINDS, include_blank: "Select" %> <br/>
  <%= f.text_field :employee_relationship, placeholder: "Relationship"  %> <br/>
  <%= f.hidden_field :_destroy %>
</fieldset>