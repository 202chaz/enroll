<div id= 'message_form'></div>
<div id="inbox_provider_form" class="table-responsive">
  <table class="table table-condensed">
    <thead>
      <tr>
        <th colspan="6">APTC / CSR Enrollments</th>
      </tr>
      <tr>
        <th>HBX ID</th>
        <th>Last Name</th>
        <th>First Name</th>
        <th>DOB</th>
        <th>SSN</th>
        <th></th>
        <th>Edit APTC/CSR</th>
      </tr>
    </thead>
    <tbody>
      <%= table_empty_to_human if families.count == 0 %>
      <% families.each do |family| %>
      <% pp = family.primary_applicant.person %>

      <tr id="family_<%= family.id %>">
        <td><%= pp.hbx_id %></td>
        <td><%= pp.last_name%></td>
        <td><%= pp.first_name%></td>
        <td><%= format_date(pp.dob) %></td>
        <td><%= begin; number_to_obscured_ssn(pp.ssn); rescue; end %></td>
        <td></td>
        <td><%= link_to 'Edit', edit_aptc_csr_path(family_id: family.id, person_id: pp.id), class: "btn btn-default btn-xs", remote: true%></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
