<table>
  <tr>
    <%= form_tag insured_verification_documents_upload_path, multipart: true, method: :post do %>
      <td>
        <input type="hidden" name="family_member" value="<%= member.id %>">
        <div class="select-upload">
          <%= select_tag :document, options_for_select_with_style, prompt: "Document type", class: "form-control" %>
        </div>
      </td>
      <td>
        <% file_field_tag :file, accept: 'image/png,image/gif,image/jpeg,application/pdf', class: "doc-upload-file", :multiple => true %>
      </td>
      <td>
        <%= submit_tag "Upload", class: "btn btn-trans" %>
      </td>
    <% end %>
  </tr>
</table>
