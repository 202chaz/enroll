<div class="row module verification-dashboard doc_upload">
  <%= form_tag insured_verification_documents_upload_path, multipart: true, method: :post do %>
      <input type="hidden" name="family_member" value="<%= member.id %>">
      <div class="col-lg-3 text-center">
        <%= select_tag :document, options_for_select(::VlpDocument::VLP_DOCUMENT_KINDS), prompt: "Select document to upload for verification" %>
      </div>
      <div class="col-lg-3 text-center">
        <%= file_field_tag :file %>
      </div>
    <div class="col-lg-3 text-center">
      <%= submit_tag "Upload", class: "btn btn-danger" %>
    </div>
  <% end %>
</div>


