<% content_for :navigation do %>
  <%= render 'navigation' %>
<% end %>

<div id="plan-summary" class="row">
</div>
<div class="row" id='account-detail'>
  <div class="my-account-page col-md-12">
    <h1>My DC Health Link</h1>
  </div>
  <div class="col-md-8">
    <%= render partial: 'qle_detail' %>
    <%= render partial: "enrollment", :collection => @hbx_enrollments, :as => :hbx_enrollment %>
    <%= render partial: 'shop_for_plans_widget' %>
    <%= render partial: 'apply_for_medicaid_widget' %>

    <% if @waived && @employee_role.present? %>
      <div class="waived-coverage">
      <%= render partial: 'waived_coverage_widget' %>
      </div>
    <% end %>

  </div>
  <div class="col-md-4">
    <%= render 'right_column' %>
  </div>
</div>
