<div class="row module verification-dashboard">
  <div class="col-md-3 text-center">
    <%= member.person.first_name %> <%= member.person.last_name%>
  </div>
  <div class="col-md-2 text-center">
    <%= show_consumer_role_state(member.person.consumer_role) %>
  </div>
  <div class="col-md-6 text-center">
    <% member.person.verification_types.each do |verif_type| %>
        <div class="row">
          <div class="col-md-4 text-center">
            <%= verif_type %>
          </div>
          <div class="col-md-4 text-center">
            <p>Verification status ???</p>
          </div>
          <div class="col-md-4 text-center">
            <%= time_remaining_in_words(member.person.created_at) %>
          </div>
        </div>
    <% end %>
  </div>
  <div class="col-md-1 text-center">
        <i class="fa fa-plus-square-o fa-2x"></i>
  </div>
</div>