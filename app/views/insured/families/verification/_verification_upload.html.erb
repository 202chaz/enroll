<div class="row">
  <div class="col-md-2">
    <button type="button" class="btn btn-default" data-container="body" data-toggle="popover" data-trigger="hover" data-placement="left" data-content="<%= info_pop_up(v_type) %>">
      <i class="fa fa-info-circle fa-1x"></i>
    </button>
  </div>
  <%= form_tag insured_verification_documents_upload_path, multipart: true, method: :post do %>
      <input type="hidden" name="family_member" value="<%= member.id %>">
      <div class="col-md-7">
        <%= file_field_tag "file[]", type: :file, accept: 'image/png,image/gif,image/jpeg,application/pdf', class: "doc-upload-file", :multiple => true %>
      </div>
      <div class="col-md-3">
        <%= submit_tag "Upload", class: "btn btn-default btn-xs" %>
      </div>
  <% end %>
</div>
