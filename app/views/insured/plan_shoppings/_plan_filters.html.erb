<div class="row all-filters-row">
	<button class="btn btn-default apply-btn interaction-click-control-plan-filter-apply">Apply</button>
	<div class="clearfix"></div>

	<div class="all-filters-detail">
	  <div class="col-md-3 col-sm-3 col-xs-6">
	    <p>Type</p>
	    <label><input type="checkbox" class="plan-type-selection-filter" data-plan-category-filter="hmo">HMO</label>
		<label><input type="checkbox" class="plan-type-selection-filter" data-plan-category-filter="ppo">PPO</label>
		<label><input type="checkbox" class="plan-type-selection-filter" data-plan-category-filter="pos">POS</label>
	  </div>
	  <div class="col-md-3 col-sm-3 col-xs-6">
	    <p>Network</p>
		<label><input type="checkbox" class="plan-metal-network-selection-filter" data-plan-metal-network-filter="nationwide">Nationwide</label>
		<label><input type="checkbox" class="plan-metal-network-selection-filter" data-plan-metal-network-filter="dc area network">DC In-Network</label>
	  </div>
	  <div class="col-md-3 col-sm-3 col-xs-6">
	    <p>Level</p>
	    <label><input type="checkbox" class="plan-metal-level-selection-filter" data-plan-metal-level-filter="bronze">Bronze</label>
			<label><input type="checkbox" class="plan-metal-level-selection-filter" data-plan-metal-level-filter="silver">Silver</label>
			<label><input type="checkbox" class="plan-metal-level-selection-filter" data-plan-metal-level-filter="gold">Gold</label>
			<label><input type="checkbox" class="plan-metal-level-selection-filter" data-plan-metal-level-filter="platinum">Platinum</label>
			<label><input type="checkbox" class="plan-metal-level-selection-filter" data-plan-metal-level-filter="catastrophic">Catastrophic</label>
	  </div>
	  <div class="col-md-3 col-sm-3 col-xs-6 no-pd">
	    <p>Premium</p>
		<label>
			<input type="text" value="$10" class='plan-metal-premium-from-selection-filter'> to <input type="text" value="$20000" class='plan-metal-premium-to-selection-filter'>
		</label>
		<p>Deductible</p>
		<label>
			<input type="text" value="$10" class='plan-metal-deductible-from-selection-filter'> to <input type="text" value="$20000" class='plan-metal-deductible-to-selection-filter'>
		</label>
	  </div>
	</div>
</div>