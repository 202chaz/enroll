#left-navigation {
  
  a:visited, a.visited {
    color: #CBE6FF;
    font-size: 14px;
    font-weight: 600;
  }

  a:hover
  {
    text-decoration: underline;
  }

  .applicant-nav a:visited {
    color: #0079C0;
    font-size: 14px;
  }

  .income-style {
    border-left: 7px solid #ffffff !important;
    a
    {
      color: #ffffff !important;
    }
  }

  .active.applicant-nav a {
    font-weight: 600;
  }

  .active.indent-w {
    border-left: 7px solid #fff !important;
  }
  .white-font {
    color: #CBE6FF;
  }

  .activer.global-nav > a {
    color: #CBE6FF;
  }

  li:not(.activer) a:not(.disabled) {
    color:#0079C0;
  }

  .active {
    border-left: 7px solid #0079C0 !important;
  }

  .global-nav.active {
    border-left: 7px solid #ffffff;
  }

  .disabled {
    background-color: white;
  }

  .global-nav.active a {
    color: #ffffff;
  }

  .active a {
    font-size: 14px;
    font-weight: 600;
  }

  .applicant-nav {
    &:first-of-type {
      border: 1px solid #c3c3c3;
    }

    background: transparent;
    a {
     background: #ffffff;
     font-weight: normal;
   }
  }

  li {
    background-color: #fff;
    border: 1px solid #c3c3c3;
    display: table;
    width: 100%;
    a {
      height: 50px;
      line-height: 18px;
      vertical-align: middle;
      display: table-cell;
    }
  }

  li:first-child {
    border: 0px;
  }

  li:nth-child(n+2) {
    border-top: 1px solid #ffffff;
  }

  li:last-child {
    border-bottom: 1px solid #c3c3c3;
  }

  .applicant-check {
    color: #69A30D;
  }
  .cna {
    cursor: not-allowed !important;
    color:#909090 !important;
  }

  .cna {
    font-weight: normal;
  }
}

.nav > li.no-hover > a:hover, .nav > li.no-hover > a:focus {
  text-decoration: none;
  background-color: #007BC4 !important;
}

.nav > li.no-hover-disable > a:hover, .nav > li.no-hover-disable > a:focus {
  text-decoration: none;
  background-color: #ffffff !important;
}

.nav > li.activer.no-hover-disable > a:hover, .nav > li.activer.no-hover-disable > a:focus {
  text-decoration: none;
  background-color: #007BC4 !important;
}
.mr-top {
  margin-top: 0px !important;
}
.bd-top
{
  border-top: none !important;
}