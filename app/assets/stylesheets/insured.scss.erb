.brokers-widget {
  .col-xs-8 {
    padding-top: 10px;
    h4 {
      padding: 15px 0 0 0;
      margin: 0;
    }
  }

  h4, .fa {
    color: #007bc4;
  }

  .col-xs-4 {
    cursor: pointer;
    &:hover {
      * {
        color: #23527c;
      }
    }
    h2 {
      margin-bottom: 0;
    }
    span {
      color: #007bc4;
    }
  }
}
.help-signup {
  h4 {
    color: #007bc4;
    margin-bottom: 0;
    font-weight: 600;
    &+small {
      display: inline-block;
      padding: 3px 0 10px;
    }
  }
}
#profile-content #address_info .mailing-div {
 display: block;
 &>.col-lg-3>* {
   display: none;
 }
}
.carrier-contact {
  padding-bottom: 20px;
  h4.modal-title {
    text-align: center;
  }
  i {
    text-align: center;
    color: #007bc4;
    display: block;
  }
  .fa-phone {
    font-size: 42px;
    padding-bottom: 10px;
  }
}
.carrier-numbers {
  text-align: center;

}
.fa-icon-link, .fa-icon-label, .fa-phone {
  color: #007bc4;
}
.fa-icon-label {
  cursor: pointer;
}
