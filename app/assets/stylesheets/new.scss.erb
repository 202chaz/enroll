#home {
  nav {
    ul {
      background-color: #007bc4;
      padding: 20px 0;
      list-style-type: none;
      img {
        width:100%
      }
      li {
        padding: 8px 0;
        &.active {
          a {
            color: #ffffff;
            border-left: solid 4px #cbe6ff;
          }
        }
        a {
          color: #cbe6ff;
          text-align: left;
          padding-left: 15px;
          font-weight: 600;
          font-size: 18px;
          display: inline-block;
          width: 100%;
          &:hover {
            background-color: transparent;
          }
        }
        span {
          background-color: #e72b2d;
        }
      }
    }
  }
  .employees-tab {
    .col-xs-12 {
      &:first-of-type {
        margin-bottom: 25px;
      }
    }
    #census_employee_search {
      margin-top: 12px;
    }
    .employees-table {
      table {
        width: 100%;
        background-color: #ffffff;
        border: solid 1px #d5d5d5;
        table-layout: fixed
      }
      th {
        padding: 12px 0;
        font-weight: normal;
        text-align: center;
      }
    }
  }
  .home-tab, .employees-tab {
    h1 {
      margin-top: 0;
      &+.panel {
        border-radius: 0;
        ul {
          list-style: square;
          font-size: 14px;
          li {
            color: #007bc4;
            margin-left: 17px;
            margin-bottom: 5px;
            &:last-of-type {
              margin-bottom: 0;
            }
            span {
              color: #8a6d3b;
              display: inline-block;
            }
            a {
              color: #007bc4;
            }
          }
        }
        &.alert {
          padding: 0;
        }
        &.panel-default {
          &>.panel-heading {
            border-radius: 0;
            background-image: none;
            background-color: #007bc4;
            border-bottom: none;
            padding: 0 20px;
            font-size: 14px;
            text-transform: uppercase;
            color: #ffffff;
            margin: 0;
            padding: 0 15px;
          }
        }
      }
    }
    .panel {
      border-radius: 0;
      border: solid 1px #d5d5d5;
      &.alert {
        padding: 0;
      }
      &.panel-default {
        &>.panel-heading {
          border-radius: 0;
          background-image: none;
          background-color: #ffffff;
          border-bottom: none;
          padding: 15px 15px 0px;
          margin: 0;
          border-bottom: solid 1px #d5d5d5;
          h3 {
            font-size: 18px;
            padding: 0;
            margin-top: 0;
            i {
              color: #007bc4;
            }
          }
          .progress {
            height: 8px;
            border: solid 1px #d5d5d5;
            border-radius: 0;
            margin-bottom: 12px;
            &+.information {
              padding-bottom: 15px;
            }
          }
        }
        .panel-body {
          i {
            &:first-of-type {
              margin-right: 10px;
            }
          }
        }
      }
      &.myplanyear, &.benefitgroup {
        border-left: solid 1px #b7b7b7;
        border-right: solid 1px #b7b7b7;
        background-color: #ffffff;
        margin-top: 0;
        padding-bottom: 20px;
        margin-bottom: 0;


        .panel-heading {
          padding: 10px 0;
        }
        &:first-of-type {
          border-top: solid 1px #b7b7b7;
        }
        &.benefitgroup {
          margin-top: 0;
          padding-top: 20px;
          &:last-of-type {
            border-bottom: solid 1px #b7b7b7;
            margin-bottom: 40px;
          }
          section {
            &:last-of-type {
              a {
                margin-top: 20px;
                float: right;
                span {
                  display: inline-block;
                }
              }
            }
            header {
              h5 {
                padding-bottom: 0;
                margin-bottom: 0;
                border: none;
              }
              h3 {
                margin: 0 0 20px 0;
                font-size: 18px;
              }
            }
          }
          table {
            tr {
              td {
                padding-bottom: 10px;
                &:last-of-type {
                  padding-left: 10px;
                  td {
                    margin-bottom: 0;
                  }
                }
              }
            }
          }
        }
        header {
          h5 {
            margin-left: 0;
            margin-bottom: 10px;
          }
        }
        table {
          width: 100%;
          tr {
            width: 100%;
            &:last-of-type {
              td {
                &:last-of-type {
                  h3 {
                    color: #69a30d;
                    font-weight: bold;
                    font-size: 14px;
                  }
                }
              }
            }
            td {
              &:first-of-type {
                header {
                  text-transform: uppercase;
                  color: #9b9b9b;
                  font-size: 12px;
                }
              }
              &:last-of-type {
                padding-right: 15px;
                h3 {
                  font-size: 22px;
                  color: #9b9b9b;
                  font-weight: semibold;
                }
              }
              * {
                margin: 0;
                padding: 0;
              }
            }
          }
        }

        h5 {
          text-transform: uppercase;
          color: #909090;
          font-weight: bold;
          margin: 0;
          font-size: 14px;
        }
        .row {
          div:first-of-type {
            padding-left: 20px;
          }

        }


      }

    }
    aside {
      &:first-of-type {
        section {
          margin-left: 5px;
          border: solid 1px #b7b7b7;
          &:first-of-type, &:last-of-type {
            margin-bottom: 20px;
            article {
              background-color: #ffffff;
              padding-top: 10px;
              margin-top: 0;
              padding-bottom: 10px;
              &:last-of-type {
                border-top: solid 1px #b7b7b7;
                figure {
                  padding: 0;
                }
              }
              figure {
                position: relative;
                span {
                  display: inline-block;
                  figcaption {
                    color: #007bc4;
                    text-transform: uppercase;
                    font-size: 12px;
                    position: relative;
                    text-align: center;
                    top: -5px;
                    h2 {
                      font-weight: bold;

                    }
                    span {
                      position: relative;
                      top:-20px;
                      display: block;
                    }

                  }
                }
              }
              h3 {
                font-size: 18px;
                margin: 0;
                line-height: 130%;
                padding-bottom: 5px;
              }
              span {
                color: #007bc4;
                text-transform: uppercase;
                font-size: 14px;
              }
              .col-xs-8 {
                &:last-of-type {
                  a {
                    &.btn {
                      background-color: #007bc4;
                      color: #ffffff;
                      border-radius: 4px;
                      border-bottom: solid 2px #004872;
                      margin-top: 10px;
                      text-transform: uppercase;
                      font-size: 16px;
                      max-width: 130px;
                    }
                  }
                }
              }
            }
          }
          &:last-of-type {
            article {
              border-bottom: solid 1px #b7b7b7;
              padding-top: 0;

              figure {
                img {
                  width: 100%;
                }
              }
              figcaption {
                padding: 0 10px 10px;
                text-align: center;
                font-size: 16px;
                h5 {
                  color: #69a30d;
                  font-weight: normal;
                  text-transform: uppercase;
                  font-size: 16px;
                  padding: 0;
                  margin-bottom: 0;
                  border-bottom: none;
                }
              }
            }
          }

        }
      }

    }



  }
}
