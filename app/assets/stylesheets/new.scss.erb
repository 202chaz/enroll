#home {
  nav {
    ul {
      background-color: #007bc4;
      padding: 20px 0;
      list-style-type: none;
      img {
        width:100%
      }
      li {
        padding: 8px 0;
        &.active {
          a {
            color: #ffffff;
            border-left: solid 4px #cbe6ff;
          }
        }
        a {
          color: #cbe6ff;
          text-align: left;
          padding-left: 15px;
          font-weight: 600;
          font-size: 18px;
          display: inline-block;
          width: 100%;
          &:hover {
            background-color: transparent;
          }
        }
        span {
          background-color: #e72b2d;
        }
      }
      &+.panel {
        border-radius: 0;
        background-image: none;
        border-bottom: none;
        padding: 0 20px;
        font-size: 14px;
        text-transform: uppercase;
        color: #ffffff;
        margin: 0;
        padding: 0 15px;
        .panel-heading {
          background-color: #ffffff;
          background-image: none;
          border-bottom: none;
          padding: 0;
          font-size: 12px;
          color: #333333;
          text-transform: capitalize;
          text-align: center;

          h4 {
            font-weight: 600;
            font-size: 18px;
            margin-bottom: 0;
            a {

            }
          }
        }
        .panel-body {
          color: #9b9b9b;
          text-transform: uppercase;
          font-size: 14px;
          text-align: center;
          .fte-count {
            color: #007bc4;
          }
      }}
    }
  }
  .employees-tab {
    .col-xs-12 {
      &:first-of-type {
        margin-bottom: 35px;
      }
      .button-group-wrapper {
        margin-bottom: 0;
      }
    }
    #census_employee_search {
      margin-top: 12px;
    }
    .filter {
      font-size: 14px;
      color: #007bc4;
      text-align: right;
      text-transform: uppercase;
      padding-top: 10px;
      cursor: pointer;
    }
    .employees-table {
        margin-top: 20px;
      table {

        width: 100%;
        table-layout: fixed;
        border: solid 1px #d5d5d5;

        tr {
        th {
          padding: 12px 0;
          font-weight: normal;
          text-align: left;
          text-transform: uppercase;
          width: 10%;
          border-bottom: solid 2px #b2b2b2;
          padding-left: 20px;
          &:hover {
            border-bottom: solid 2px #007bc4;

          }

          }
          td {
            background-color: #ffffff;
            padding: 10px 20px;
            text-align: left;
            border-bottom: solid 1px #b2b2b2;
            font-size: 13px;
            height: 50px;

            &:first-of-type {
              font-size: 15px;
            }
            &:last-of-type {
              text-align: right;
            }
            &.table-functions .fa {
              display: none;
              cursor: pointer;
              &:first-of-type {
                padding-right: 10px;
              }
            }
            &:hover {
              border-bottom: solid 2px #007bc4;

            }

          }
          &:hover .table-functions .fa {
            color: #007bc4;
            display: inline-block;
          }
        }




      }

    }
  }
  .planyear-add-tab {
    h3 {
      &:first-of-type {
        display: inline-block;
        margin-top: 0;
        &+h4 {
          display: inline-block;
        }
      }
    }
    .row-form-wrapper {
      border-top: solid 1px #b7b7b7;
      margin-bottom: 10px;
    }
    .recommend {
      .panel {
        padding-bottom: 15px;
        .panel-heading {
          text-transform: uppercase;
          h3 {
            font-weight: bold;
            color: #333333;
            font-size: 18px;
            margin: 0;
          }
        }
        .panel-body {
          padding: 0px inherit;
          p {
            margin: 0;
          }
        }

        .col-xs-3 {
          padding-top: 15px;
          span {
            display: inline-block;
            text-align: center;
            h3.day {
              padding-top: 15px;
              display: block;
              font-weight: 600;
              font-size: 24px;
              margin: 0;

            }
            span.month {
              text-transform: uppercase;
              font-size: 12px;
              display: block;
              position: relative;
              top:-5px;

            }
          }
        }
      }
    }
  }
  .home-tab, .employees-tab, .employee-add-tab, .benefits-tab,
  .planyear-add-tab, .brokers-tab, .employer-tab, .employer-edit-tab {
    h1 {
      margin-top: 0;
      color: #003260;
      &+.panel {
        border-radius: 0;
        ul {
          list-style: square;
          font-size: 14px;
          li {
            color: #007bc4;
            margin-left: 17px;
            margin-bottom: 5px;
            &:last-of-type {
              margin-bottom: 0;
            }
            span {
              color: #8a6d3b;
              display: inline-block;
            }
            a {
              color: #007bc4;
            }
          }
        }
        &.alert {
          padding: 0;
        }
        &.panel-default {
          &>.panel-heading {
            border-radius: 0;
            background-image: none;
            background-color: #007bc4;
            border-bottom: none;
            padding: 0 20px;
            font-size: 14px;
            text-transform: uppercase;
            color: #ffffff;
            margin: 0;
            padding: 0 15px;
          }
        }
      }
    }
    .panel {
      border-radius: 0;
      border: solid 1px #d5d5d5;
      &.alert {
        padding: 0;
      }
      &.panel-default {
        &>.panel-heading {
          border-radius: 0;
          background-image: none;
          background-color: #ffffff;
          border-bottom: none;
          padding: 15px 15px 0px;
          margin: 0;
          border-bottom: solid 1px #d5d5d5;
          h3 {
            font-size: 18px;
            padding: 0;
            margin-top: 0;
            i {
              color: #007bc4;
            }
          }
          .progress {
            height: 8px;
            border: solid 1px #d5d5d5;
            border-radius: 0;
            margin-bottom: 12px;
            &+.information {
              padding-bottom: 15px;
            }
          }
        }
        .panel-body {
          i {
            &:first-of-type {
              margin-right: 10px;
            }
          }
        }
      }
      &.myplanyear, &.benefitgroup {
        border-left: solid 1px #b7b7b7;
        border-right: solid 1px #b7b7b7;
        background-color: #ffffff;
        margin-top: 0;
        padding-bottom: 20px;
        margin-bottom: 0;


        .panel-heading {
          padding: 10px 0;
        }
        &:first-of-type {
          border-top: solid 1px #b7b7b7;
        }
        &.benefitgroup {
          margin-top: 0;
          padding-top: 20px;
          &:last-of-type {
            border-bottom: solid 1px #b7b7b7;
            margin-bottom: 40px;
          }
          section {
            &:last-of-type {
              a {
                margin-top: 20px;
                float: right;
                span {
                  display: inline-block;
                }
              }
            }
            header {
              h5 {
                padding-bottom: 0;
                margin-bottom: 0;
                border: none;
              }
              h3 {
                margin: 0 0 20px 0;
                font-size: 18px;
              }
            }
          }
          table {
            tr {
              td {
                padding-bottom: 10px;
                &:last-of-type {
                  padding-left: 10px;
                  td {
                    margin-bottom: 0;
                  }
                }
              }
            }
          }
        }
        header {
          h5 {
            margin-left: 0;
            margin-bottom: 10px;
          }
        }
        table {
          width: 100%;
          tr {
            width: 100%;
            &:last-of-type {
              td {
                &:last-of-type {
                  h3 {
                    color: #69a30d;
                    font-weight: bold;
                    font-size: 14px;
                  }
                }
              }
            }
            td {
              &:first-of-type {
                header {
                  text-transform: uppercase;
                  color: #9b9b9b;
                  font-size: 12px;
                }
              }
              &:last-of-type {
                padding-right: 15px;
                h3 {
                  font-size: 22px;
                  color: #9b9b9b;
                  font-weight: semibold;
                }
              }
              * {
                margin: 0;
                padding: 0;
              }
            }
          }
        }

        h5 {
          text-transform: uppercase;
          color: #909090;
          font-weight: bold;
          margin: 0;
          font-size: 14px;
        }
        .row {
          div:first-of-type {
            padding-left: 20px;
          }

        }


      }

    }
    aside {
      &:first-of-type {
        section {
          margin-left: 5px;
          border: solid 1px #b7b7b7;
          &:first-of-type, &:last-of-type {
            margin-bottom: 20px;
            article {
              background-color: #ffffff;
              padding-top: 10px;
              margin-top: 0;
              padding-bottom: 10px;
              &:last-of-type {
                border-top: solid 1px #b7b7b7;
                figure {
                  padding: 0;
                }
              }
              figure {
                position: relative;
                span {
                  display: inline-block;
                  figcaption {
                    color: #007bc4;
                    text-transform: uppercase;
                    font-size: 12px;
                    position: relative;
                    text-align: center;
                    top: -5px;
                    h2 {
                      font-weight: bold;

                    }
                    span {
                      position: relative;
                      top:-20px;
                      display: block;
                    }

                  }
                }
              }
              h3 {
                font-size: 18px;
                margin: 0;
                line-height: 130%;
                padding-bottom: 5px;
              }
              span {
                color: #007bc4;
                text-transform: uppercase;
                font-size: 14px;
              }
              .col-xs-8 {
                &:last-of-type {
                  a {
                    &.btn {
                      background-color: #007bc4;
                      color: #ffffff;
                      border-radius: 4px;
                      border-bottom: solid 2px #004872;
                      margin-top: 10px;
                      text-transform: uppercase;
                      font-size: 16px;
                      max-width: 130px;
                    }
                  }
                }
              }
            }
          }
          &:last-of-type {
            article {
              border-bottom: solid 1px #b7b7b7;
              padding-top: 0;

              figure {
                img {
                  width: 100%;
                }
              }
              figcaption {
                padding: 0 10px 10px;
                text-align: center;
                font-size: 16px;
                h5 {
                  color: #69a30d;
                  font-weight: normal;
                  text-transform: uppercase;
                  font-size: 16px;
                  padding: 0;
                  margin-bottom: 0;
                  border-bottom: none;
                }
              }
            }
          }

        }
      }

    }


    .btn {
      &.btn-trans {
        border: solid 1px #b2b2b2;
        font-weight: bold;
        font-size: 16px;
        color: #007bc4;
        border-radius: 4px;
        padding: 5px 20px;
        &:first-of-type {
          margin-right: 15px;
        }

        &:hover {
          background-color: #ffffff;
          border: solid 1px #007bc4;
        }
      }
    }
    .fa {
      color: #007bc4;
    }
  }

}
