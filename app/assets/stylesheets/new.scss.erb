        width: 100%;
      }
      .row-form-wrapper.heading {
        text-transform: uppercase;
        font-size: 14px;
        padding: 10px 0;

        color: rgb(131, 135, 128);
        border-bottom: none;
        border-left: solid 1px #d5d5d5;
        border-right: solid 1px #d5d5d5;

        &+.benefits-fields {
          background-color: #ffffff;
          border-left: solid 1px #d5d5d5;
          border-right: solid 1px #d5d5d5;
          border-bottom: solid 1px #d5d5d5;
