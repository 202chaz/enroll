            .slider-selection {
               background: #9C9C9C;
            }
          }

          .row-form-wrapper {
            border-bottom: solid 1px #d5d5d5;
            &:last-of-type {
              border-bottom: none;
            }
          }

        }
        .col-xs-6 {
          border-bottom: solid 1px #d5d5d5;
          padding: 15px 0 25px;
          text-align: center;

        }
        .row-form-wrapper {
          border: none;
          border-left: solid 1px #d5d5d5;
          border-top: none;

        }
        &+.row-form-wrapper {
            border-top: none;


        }
      }
    .row-form-wrapper {
      border: none;
    }

    }
    ul.nav.nav-tabs, .plan-options .col-xs-3 a {
      border-bottom: none;
      li {
        a {
          padding: 0;
        }

        &.active {
          label {
            border: solid 1px #b7b7b7;
            border-bottom: none;
            background-color: #ffffff;
            border-bottom-left-radius: 0px;
            border-bottom-right-radius: 0px;
            z-index: 100;

            color: #333333;
            &:hover {
              background-color: #ffffff;
            }
          }
        }

          label {
            font-weight: bold;
            font-size: 18px;
            margin-right: 10px;
            padding: 10px;
            background-color: #007bc4;
            border-bottom: solid 3px #003260;
            border-radius: 6px;
            width: 100%;
            height: 100%;
            color: #ffffff;
            margin-bottom: 0;
            text-transform: uppercase;
            &:hover {
              background-color: #003260;
              cursor: pointer;
            }
            i {
              padding-right: 15px;
            }

          }

      }
      input[type=radio] {
        display: none;
      }
      input[type=radio] + label {

        &>i {
          font-size: 28px;
          color: #b7b7b7;



          &:first-of-type {
            display: inline-block;

          }
          &:last-of-type {
            display: none;
          }
        }
      }
      input[type=radio]:checked + label {

        &>i {
          color: #69A30D;
          &:first-of-type {
            display: none;
          }
          &:last-of-type {
            display: inline-block;
          }
        }
      }
    }
    .plan-options {
      background-color: #ffffff;
      border: solid 1px #b7b7b7;
      span:first-of-type {
        text-align: center;
        font-size: 16px;
        display: block;
        padding: 40px 0;
        margin: auto;
        max-width: 660px;
      }
      .col-xs-3 {

      }
      .col-xs-9 {
        padding-right: 0;
        padding-left: 10px;
        .panel {
          border: none;
          -webkit-box-shadow: none;
          box-shadow: none;
          .panel-heading {

            padding: 0;
            border-bottom: none;

            h3 {
              font-size: 16px;
              padding-top: 2px;
              font-weight: bold;
              text-transform: capitalize;


            }


          }
          .panel-body {

          }
        }
      }
      &+.reference-plans {
        background-color: #ffffff;
        border: solid 1px #d7d7d7;
        padding-bottom: 25px;
        overflow-y: scroll;


        .loading {
          position: relative;
          padding-top: 25px;
          top: 50%;
          text-align: center;
        }
        .reference-plan {
          height: 150px;
          padding: 30px 0 0 0;
          border-bottom: solid 1px #b7b7b7;
          &:hover {
            border-bottom: solid 1px #007bc4;
          }
          .col-xs-3:first-of-type {
            padding-left: 0;
          }
          .panel {
            -webkit-box-shadow: none;
            box-shadow: none;
            border: none;
            .panel-heading {
              padding: 0 0 5px 0;
              color: #333333;
              h3 {
                font-size: 12px;
                weight: 700;
                margin-bottom: 0;
              }
            }
            .panel-body {
              padding: 0;

              span {
                font-size: 12px;
                color: #333333;
                text-transform: uppercase;
              }
              .plan-label {
                color: #b7b7b7;
                font-size: 10px;

              }
            }
          }

          input[type=radio] {
            display: none;
          }
          input[type=radio] + label {

            &>i {
              font-size: 28px;
              color: #b7b7b7;



              &:first-of-type {
                display: inline-block;

              }
              &:last-of-type {
                display: none;
              }
            }
          }
          input[type=radio]:checked + label {

            &>i {
              color: #69A30D;
              &:first-of-type {
                display: none;
              }
              &:last-of-type {
                display: inline-block;
              }
            }
          }

        }


      }
    }


    input[type=number]::-webkit-inner-spin-button,
    input[type=number]::-webkit-outer-spin-button {
      -webkit-appearance: none;
      margin: 0;

    }
    input[type=radio] {
      display: inline-block;
    }

   input[type=checkbox] {
      display: none;
      + label > i.fa {
        color: #69A30D;
      }
    }
    .checkbox {
      display: block;
      margin: auto;
      padding-left: 15%;
    }
    .checkbox {
      &>div {
        display: inline-block;
        margin: auto;
      }
    }
    .checkbox label {
      padding-left: 0;
    }
    .checkbox label label {
      display: inline;
      padding-left: 0;
    }
    .checkbox label label + span {
      display: inline-block;
      position: relative;
      text-transform: capitalize;
      font-size: 16px;
      top: -5px;
      left: 10px;
    }
    input[type=checkbox]:disabled + label i {
      color: #b7b7b7;
      cursor: not-allowed;
    }
    input[type=checkbox] + label i {
      &:first-of-type {
        display: none;

      }
      &:last-of-type {
        display: inline-block;
        width: 30px;
      }
    }
    input[type=checkbox]:checked + label i {
      &:first-of-type {
        display: inline-block;
        width: 30px;
      }
      &:last-of-type {
        display: none;
      }
    }

  }
