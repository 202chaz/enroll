#home {
  nav {
    ul {
      background-color: #007bc4;
      padding: 0;
      list-style-type: none;
      img {
        width:100%
      }
      li {
        padding: 8px 0;
        &.active {
          a {
            color: #ffffff;
            border-left: solid 4px #cbe6ff;
          }
        }
        a {
          color: #cbe6ff;
          text-align: center;
          font-weight: Semibold;
          font-size: 18px;
          display: inline-block;
          width: 100%;
          &:hover {
            background-color: transparent;
          }
        }
        span {
          background-color: #e72b2d;
        }
      }
    }
  }
  .home-tab {
    .panel {
      border-radius: 0;
      &.alert {
        padding: 0;
      }
      &.panel-default {
        &>.panel-heading {
          border-radius: 0;
          background-image: none;
          background-color: #007bc4;
          border-bottom: none;
          padding: 0 20px;
          font-size: 14px;
          text-transform: uppercase;
          color: #ffffff;
          margin: 0;
        }
      }
    }
  }
}
