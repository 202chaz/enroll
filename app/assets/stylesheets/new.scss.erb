#home {
  nav {
    ul {
      background-color: #007bc4;
      padding: 20px 0;
      list-style-type: none;
      img {
        width:100%
      }
      li {
        padding: 8px 0;
        &.active {
          a {
            color: #ffffff;
            border-left: solid 4px #cbe6ff;
          }
        }
        a {
          color: #cbe6ff;
          text-align: left;
          padding-left: 15px;
          font-weight: 600;
          font-size: 18px;
          display: inline-block;
          width: 100%;
          &:hover {
            background-color: transparent;
          }
        }
        span {
          background-color: #e72b2d;
        }
      }
      &+.panel {
        border-radius: 0;
        background-image: none;
        border-bottom: none;
        padding: 0 20px;
        font-size: 14px;
        text-transform: uppercase;
        color: #ffffff;
        margin: 0;
        padding: 0 15px;
        .panel-heading {
          background-color: #ffffff;
          background-image: none;
          border-bottom: none;
          padding: 0;
          font-size: 12px;
          color: #333333;
          text-transform: capitalize;
          text-align: center;

          h4 {
            font-weight: 600;
            font-size: 18px;
            margin-bottom: 0;
            a {

            }
          }
        }
        .panel-body {
          color: #9b9b9b;
          text-transform: uppercase;
          font-size: 14px;
          text-align: center;
          .fte-count {
            color: #007bc4;
          }
      }}
    }
  }
  .employees-tab {
    .col-xs-12 {
      &:first-of-type {
        margin-bottom: 35px;
      }
      .button-group-wrapper {
        margin-bottom: 0;
      }
    }
    #census_employee_search {
      margin-top: 12px;
    }
    #employees-filter {
      position: relative;
      i {
        color: #007bc4;
      }
    }
    .filter {
      font-size: 14px;
      color: #007bc4;
      text-align: right;
      text-transform: uppercase;
      padding-top: 10px;
      cursor: pointer;

    }
    .filter-options {
      display: none;
      background-color: #fff;
      border: solid 1px #b7b7b7;
      padding: 5px 15px 30px 20px;
      position: absolute;
      right: 0;
      top: 0;
      color: #23527c;


      h4 {
        text-align: right;
        text-transform: uppercase;
        font-size: 14px;
        color: #333;
      }
      input {
      }
      label {
        display: block;
        text-align: right;
        color: #007bc4;
        font-size: 18px;
        padding: 0;
        width: 100%;
        &.btn {
          display: block;
          width: 100%;
          background-color: transparent;
          background-image: none;
          border: none;
          border-color: none;
        }
        &.btn-default {
          display: block;
          width: 100%;
          background-color: transparent;
          background-image: none;
          border: none;
          border-color: none;
          &:hover, &:active, &:focus, &:active {
            background-color: transparent;
            border-color: none;
            -webkit-box-shadow: none;
            box-shadow: none;
          }
        }
        &.active {
          background-color: transparent;
          background-image: none;
          border: none;
          -webkit-box-shadow: none;
          box-shadow: none;

        }
        &:hover {
          color: underline;
        }
      }


    }
    .employees-table {
        margin-top: 20px;
      table {

        width: 100%;
        table-layout: fixed;
        border: solid 1px #d5d5d5;

        tr {
        th {
          padding: 12px 0;
          font-weight: normal;
          text-align: left;
          text-transform: uppercase;
          width: 10%;
          border-bottom: solid 2px #b2b2b2;
          padding-left: 20px;
          &:hover {
            border-bottom: solid 2px #007bc4;

          }

          }
          td {
            background-color: #ffffff;
            padding: 10px 20px;
            text-align: left;
            border-bottom: solid 1px #b2b2b2;
            font-size: 13px;
            height: 50px;

            &:first-of-type {
              font-size: 15px;
            }
            &:last-of-type {
              text-align: right;
            }
            &.table-functions .fa {
              display: none;
              cursor: pointer;
              &:first-of-type {
                padding-right: 10px;
              }
            }
            &:hover {
              border-bottom: solid 2px #007bc4;

            }

          }
          &:hover .table-functions .fa {
            color: #007bc4;
            display: inline-block;
          }
        }




      }

    }
  }
  #add_employee_action {
    .col-xs-12 {
      .col-md-6 {
        height: 180px;
      }
    }
  }
  .planyear-add-tab {
    h3 {
      &:first-of-type {
        display: inline-block;
        margin-top: 0;
        &+h4 {
          display: inline-block;
        }
      }
    }
    .fa {
      color: #007bc4;
      cursor: pointer;
    }
    .row-form-wrapper {
      border-top: solid 1px #b7b7b7;
      margin-bottom: 10px;
    }
    .recommend {
      .panel {
        padding-bottom: 15px;
        background-color: transparent;
        .panel-heading {
          text-transform: uppercase;
          background-color: transparent;

          h3 {
            font-weight: bold;
            color: #333333;
            font-size: 18px;
            margin: 0;
          }
        }
        .panel-body {
          padding: 0px inherit;
          p {
            margin: 0;
          }
        }

        .col-xs-3 {
          padding-top: 15px;
          span {
            display: inline-block;
            text-align: center;

            h3.day {
              padding-top: 15px;
              display: block;
              font-weight: 600;
              font-size: 24px;
              margin: 0;

            }
            span.month {
              text-transform: uppercase;
              font-size: 12px;
              display: block;
              position: relative;
              top:-5px;

            }
          }
        }
      }
    }

  }
  .benefits-tab {
    .plan-details {
      display: none;
    }
    .details {
      cursor: pointer;
    }
    .package-offering {
      border: solid 1px #b7b7b7;

      background-color: #ffffff;
      padding: 10px 0 20px;
      border-bottom: solid 3px #b7b7b7;

    table {
      width: 96%;
      padding: 0 2%;
      margin: auto;
      tr:first-of-type {
        border-top: none;
