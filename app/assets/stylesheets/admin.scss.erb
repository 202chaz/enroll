.module {
  &.edit-aptc-csr-table {

    td.historic-aptc-csr-data {
      background-color:#ffd6cc;
    }

    td.current-aptc-csr-data {
      background-color:#e6fff3;
    }

    td.aptc-csr-grid-separator {
      background-color:#F2F2F2;
      padding: 2px;
      border: #F2F2F2;
    }

    .month-labels {
      padding: 8px 10px !important;
    }
  }

  &.filter-header {
    td{
      padding-TOP: 10px;
      padding-BOTTOM: 0px; 
    }

    .selectric-wrapper {
      max-width:35%;
    }
    .label {
      font-weight: bold;
      font-size: 15px;
    }
    .glyphicon {
      top: 12px;
      padding-RIGHT: 0px;
    }

  }


}

